package tato_bot_commands;

import kong.unirest.HttpResponse;
import kong.unirest.Unirest;
import net.dv8tion.jda.api.EmbedBuilder;
import net.dv8tion.jda.api.events.message.guild.GuildMessageReceivedEvent;
import net.dv8tion.jda.api.hooks.ListenerAdapter;
import tato_bot.main;

public class Search extends ListenerAdapter {
	
	public void onGuildMessageReceived (GuildMessageReceivedEvent event)
	{
		String[] args = event.getMessage().getContentRaw().split("\\s+");
		
		if (args[0].equalsIgnoreCase(main.prefix + "search"))
		{
			HttpResponse<String> response = Unirest.get("https://contextualwebsearch-websearch-v1.p.rapidapi.com/api/Search/ImageSearchAPI?autoCorrect=false&pageNumber=1&pageSize=10&q=Taylor%20Swift&safeSearch=false")
					.header("x-rapidapi-host", "contextualwebsearch-websearch-v1.p.rapidapi.com")
					.header("x-rapidapi-key", "ca2d4917cdmsh8a43388ab8c5278p1e2e89jsn87def1dcf18e")
					.asString();
			
			EmbedBuilder search = new EmbedBuilder();
			
			search.setImage();
			
		}
	}

}
